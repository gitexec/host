<p id="notice"><%= notice %></p>

<h1>Listing Engine1s</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @engine1s.each do |engine1| %>
      <tr>
        <td><%= engine1.title %></td>
        <td><%= link_to 'Show', engine1 %></td>
        <td><%= link_to 'Edit', edit_engine1_path(engine1) %></td>
        <td><%= link_to 'Destroy', engine1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Engine1', new_engine1_path %>
